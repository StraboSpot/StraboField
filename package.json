{
  "name": "strabospot2",
  "version": "1.45.3",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "ios": "npx react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "upload-sourcemaps-ios": "sentry-cli releases files org.StraboSpot2-1.44.2 upload-sourcemaps ./main.jsbundle ./main.jsbundle.map --dist org.StraboSpot2-1.44.2",
    "upload-sourcemaps-android": "sentry-cli releases files org.StraboSpot2-1.44.2 upload-sourcemaps index.android.bundle index.android.bundle.map --dist org.StraboSpot2-1.44.2",
    "upload-sourcemaps": "concurrently --kill-others \"npm:upload-sourcemaps-android\" \"npm:upload-sourcemaps-ios\"",
    "build:ios": "react-native bundle --entry-file index.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map  --dev false --platform ios --sourcemap-output ios/main.jsbundle.map --reset-cache",
    "build:android": "react-native bundle --entry-file index.js --bundle-output android/index.android.bundle --minify true --dev false --platform android --sourcemap-output android/index.android.bundle.map --reset-cache",
    "deploy:android": "cd android && ./gradlew bundleRelease",
    "bump-patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && bundle exec fastlane bump",
    "deploy-beta": "bundle exec fastlane beta",
    "web": "webpack serve --mode=development --config webpack.config.js"
  },
  "scriptComments": {
    "build:android": "Must be run with every build for android before deploying to PlayStore. Removed the --assets flag because of duplicates.",
    "deploy:android": "1. run build:android && cd android && ./gradlew bundleRelease (creates a .aab file in app/build/outputs/bundle/release)"
  },
