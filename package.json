{
  "name": "strabospot2",
  "version": "1.47.5",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android-release": "react-native run-android --mode=release",
    "ios-release": "react-native run-ios --mode=Release",
    "ios": "npx react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "upload-sourcemaps-ios": "sentry-cli releases files org.StraboSpot2-1.47.4 upload-sourcemaps ./main.jsbundle ./main.jsbundle.map --dist org.StraboSpot2-1.47.4",
    "upload-sourcemaps-android": "sentry-cli releases files org.StraboSpot2-1.47.4 upload-sourcemaps index.android.bundle index.android.bundle.map --dist org.StraboSpot2-1.47.4",
    "upload-sourcemaps": "concurrently --kill-others \"npm:upload-sourcemaps-android\" \"npm:upload-sourcemaps-ios\"",
    "bundle:ios": "react-native bundle --entry-file index.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map  --dev false --platform ios --sourcemap-output ios/main.jsbundle.map --reset-cache",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/index.android.bundle.map --assets-dest android/app/src/main/res --reset-cache && rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw ",
    "deploy:android": "npx react-native build-android --mode=release",
    "bump-patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && bundle exec fastlane bump",
    "deploy-beta": "bundle exec fastlane beta",
    "web": "webpack serve --mode=development --config webpack.config.js"
  },
  "scriptComments": {
    "build:android": "Must be run with every build for android before deploying to PlayStore. Removed the --assets flag because of duplicates.",
    "deploy:android": "1. run build:android && cd android && ./gradlew bundleRelease (creates a .aab file in app/build/outputs/bundle/release)"
  },
